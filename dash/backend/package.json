{
  "name": "m9sweeper-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "cli": "nestjs-command",
    "prebuild": "rimraf dist",
    "copy-misc-vars:local": "cp src/config/licensing-portal-config/misc.local.ts src/config/misc.ts",
    "copy-misc-vars:dev": "cp src/config/licensing-portal-config/misc.dev.ts src/config/misc.ts",
    "copy-misc-vars:prod": "cp src/config/licensing-portal-config/misc.prod.ts src/config/misc.ts",
    "build:api": "nest build",
    "build:local": "npm run copy-misc-vars:local && npm run build:api",
    "build:prod": "npm run copy-misc-vars:prod && npm run build:api && cp package*.json ./dist/ && cp info.json ./dist/",
    "build:prod:webpack": "npm run copy-misc-vars:prod && nest build --webpack webpack.config.js && npx tsc ./src/migrations/*.ts --outDir ./dist/migrations -t es5 && npx tsc ./src/seeds/*.ts --outDir ./dist/seeds -t es5",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:local": "npm run copy-misc-vars:local && npm run start:watch",
    "start:local:dev-db": "cp dev-db.env .env && npm run start:local",
    "start:dev-config": "npm run copy-misc-vars:dev && cp dev-db.env .env && nest start --watch",
    "start:prod:simulate": "node dist/main",
    "start:prod": "node main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cli:cluster:sync-all": "nestjs-command cluster:sync all",
    "cli:exceptions:init": "nestjs-command exceptions:init",
    "cli:registries:init": "nestjs-command registries:init",
    "cli:users:init": "nestjs-command users:init",
    "cli:populate:kubernetes-history": "nestjs-command populate:kubernetes-history",
    "cli:cluster:seed": "nestjs-command cluster:seed",
    "cli:init:all": "npm run cli:users:init && npm run cli:cluster:seed && npm run cli:registries:init && npm run cli:exceptions:init && npm run cli:cluster:sync-all",
    "cli:database:status": "nestjs-command database:status"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.12.0",
    "@nestjs-modules/mailer": "file:vendor/nestjs-mailer",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.3",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/platform-express": "^9.3.3",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@willsoto/nestjs-prometheus": "^0.1.3",
    "amqp-connection-manager": "^3.2.2",
    "amqplib": "^0.8.0",
    "aws-sdk": "^2.710.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "client-oauth2": "^4.3.0",
    "copy-webpack-plugin": "^6.4.1",
    "date-fns": "^2.28.0",
    "docker-registry-api": "0.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "glob": ">=7.1.7",
    "handlebars": "^4.7.6",
    "js-yaml": "^4.1.0",
    "knex": "^2.0.0",
    "knexnest": "file:vendor/knexnest",
    "ldapjs": "^2.3.3",
    "lodash": "^4.17.21",
    "multer": "1.4.4-lts.1",
    "multer-s3": "^2.9.0",
    "nest-router": "^1.0.9",
    "nest-winston": "^1.8.0",
    "nestjs-command": "^3.1.3",
    "nodemailer": "^6.4.10",
    "pg": "^8.2.1",
    "prom-client": "^12.0.0",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.1.4",
    "tableify": "^1.1.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "uuidv4": "^6.2.13",
    "winston": "^3.3.3",
    "ws": "^7.3.1",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^9.3.9",
    "@types/amqplib": "^0.8.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "^27.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "18.14.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.12",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "^28.1.0",
    "prettier": "^1.19.1",
    "supertest": "^6.2.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
